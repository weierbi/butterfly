V0：
#include <stdio.h>
#include<string.h>
void  main() 	
{
 char str[ ]="3+4" ;

int left= str[0] - '0' ;
int right = str[2] - '0' ;
int res = left + right;
printf("res = %d\n" ,res) ;

return 0;
}


V1：
#include <stdio.h>
#include<string.h>
void  main() 	
{
char str[]="1+2+2+1+2+5+4-1-3+4-8";
int res=str[0]-'0' ;
for(int i=1;i<strlen(str);i++)
{
if (str[i]='+')
{int right=str[i+1]-'0';
res=res+right;
i++;
}
else if(str[i]=='-')
{
int right=str[i+1]-'0';
res=res-right;

i++ ;
}
}
printf("res =%d",res);

return 0;
}


V2：
#include <stdio.h>
#include<string.h>
void  main() 	
{
 char str[]="2*2/4*1/1*2*3/2";
int res=str[0]-'0' ;
for(int i=1;i<strlen(str);i++)
{
if (str[i]-='*')
int right=str[i+1]-'0';
res=res*right;
i++;
}
else if(str[i]=='/')
{
int right=str[i+1]-'0';
res=res/right;

i++ ;
}
printf("res =%d",res);

return 0;
}



V3：
#include <stdio.h>
#include<string.h>
void  main() 	
{
char str[]="2+2*3+2/2-1";
char Tmp[strlen(str)];
int TmpIndex=-1;
for(int i=0 ;i<strlen(str);i++)
{
   if(str[i]=='*')
{
int left=Tmp[TmpIndex] - '0';
int right=str[i+1]-'0' ;
Tmp[TmpIndex]=left*right+ '0';  i++;
}
else if(str[i]=='/')
{
int left=Tmp[TmpIndex] - '0';
int right=str[i+1] - '0'
Tmp[TmpIndex]=left/right+ '0';
i++;
}
else Tmp[++TmpIndex]-str[i];
}
int res=str[0];
for(int i=1;i<strlen(str);i++)
{
if (str[i]='+')
{int m=str[i+1]-'0';
res=res+m;  i++;
}
else if(str[i]=='-')
{
int m=str[i+1]-'0';
res=res-m;  i++ ;
}
}
printf("res =%d",res);
return 0;
}
    
